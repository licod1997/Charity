                                    <script type="text/javascript">
                                        var currentPage = parseInt('<c:out value="${CurrentPage}"/>');
                                        var maxPage = parseInt('<c:out value="${TotalPages}"/>');
                                        $(document).ready(function () {
                                            if (maxPage > 4) {
                                                if (currentPage - 2 < 2) {
                                                    for (var i = 1; i <= 5; i++) {
                                                        writeHTML(i);
                                                    }
                                                    return;
                                                } else if (currentPage + 2 > maxPage - 1) {
                                                    for (var i = maxPage - 4; i <= maxPage; i++) {
                                                        writeHTML(i);
                                                    }
                                                    return;
                                                } else {
                                                    for (var i = currentPage - 2; i <= currentPage + 2; i++) {
                                                        writeHTML(i);
                                                    }
                                                    return;
                                                }
                                            }
                                            if (maxPage <= 4) {
                                                for (var i = 1; i <= maxPage; i++) {
                                                    writeHTML(i);
                                                }
                                            }
                                        });
                                        function writeHTML(i) {
                                            if (i === currentPage) {
                                                $("ul.pager").append('<li><a rel="nofollow" href="/news?page=' + i + '"><b>' + i + '</b></a></li>');
                                            } else {
                                                $("ul.pager").append('<li><a rel="nofollow" href="/news?page=' + i + '">' + i + '</a></li>');
